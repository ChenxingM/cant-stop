{
  "onebot": {
    "url": "http://127.0.0.1:3001",
    "timeout": 30
  },
  "bot": {
    "listen_host": "127.0.0.1",
    "listen_port": 8080,
    "allowed_groups": [],
    "admin_users": [],
    "auto_register": true
  },
  "message": {
    "use_emoji": true,
